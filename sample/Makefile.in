CPPFLAGS += -I@prefix@/include 
LDFLAGS  += -L@prefix@/lib -lcam1394  @LIBRAW1394_LIBS@
CPPFLAGS += @X_CFLAGS@
LDFLAGS  += @X_PRE_LIBS@ @X_LIBS@ -lX11

